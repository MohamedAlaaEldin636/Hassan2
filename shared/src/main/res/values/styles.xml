<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="FabAsRect">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">8dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSizeSmall" parent="">
        <item name="cornerSize">16dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSizeSmall2" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeTopLeft">16dp</item>
        <item name="cornerSizeBottomLeft">16dp</item>
        <item name="cornerSizeBottomRight">16dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSizeTiny" parent="">
        <item name="cornerSize">16dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSizeTiny2" parent="">
        <item name="cornerSize">8dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSizeTiny3" parent="">
        <item name="cornerSize">4dp</item>
    </style>

    <style name="ToolbarColoredBackArrow" parent="@style/Theme.HassanU">
        <item name="android:textColorSecondary">@color/black</item>
    </style>

    <style name="TransparentDialogTheme">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="ScaleDialogAnim">
        <item name="android:windowEnterAnimation">@anim/scale_in</item>
        <item name="android:windowExitAnimation">@anim/scale_out</item>
    </style>

    <style name="ShapeAppearanceOverlay.MaterialCardView.RightOnly" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">8dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeBottomRight">8dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.MaterialCardView.TopOnly" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">8dp</item>
        <item name="cornerSizeTopLeft">8dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.MaterialCardView.V1" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">1000dp</item>
        <item name="cornerSizeTopLeft">1000dp</item>
        <item name="cornerSizeBottomRight">1000dp</item>
        <item name="cornerSizeBottomLeft">1000dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.MaterialCardView.LeftOnly" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeTopLeft">8dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeBottomLeft">8dp</item>
    </style>
